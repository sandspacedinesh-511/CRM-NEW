{
  "name": "counselor-crm-backend",
  "version": "1.0.0",
  "description": "Backend API for Counselor CRM system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "node scripts/migrateDatabase.js",
    "seed": "node scripts/seedDatabase.js",
    "create-admin": "node scripts/createAdminUser.js",
    "health-check": "node healthcheck.js",
    "build": "npm ci --only=production",
    "pm2:start": "pm2 start pm2.config.js --env production",
    "pm2:stop": "pm2 stop counselor-crm",
    "pm2:restart": "pm2 restart counselor-crm",
    "pm2:reload": "pm2 reload counselor-crm",
    "test-digitalocean": "node scripts/testDigitalOcean.js",
    "test-digitalocean-simple": "node scripts/simpleDigitalOceanTest.js",
    "test-minimal": "node scripts/minimalTest.js",
    "test-aws-cli": "node scripts/awsCliTest.js",
    "test-add-student": "node scripts/testAddStudent.js",
    "migrate-to-digitalocean": "node scripts/migrateToDigitalOcean.js",
    "pm2:logs": "pm2 logs counselor-crm",
    "pm2:monit": "pm2 monit",
    "docker:build": "docker build -t counselor-crm-backend .",
    "docker:run": "docker run -p 3000:3000 --env-file .env counselor-crm-backend"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.883.0",
    "@aws-sdk/s3-request-presigner": "^3.883.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cluster": "^0.7.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "express-rate-limit": "^6.10.0",
    "express-slow-down": "^1.6.0",
    "express-validator": "^7.0.1",
    "express-winston": "^4.2.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.4",
    "pm2": "^5.3.0",
    "redis": "^4.6.7",
    "sequelize": "^6.32.1",
    "socket.io": "^4.7.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
